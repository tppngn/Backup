    ethernet eth0 {
        address 190.14.171.253/30
        address 172.18.18.10/32
        description Lnk-TELECOM
        duplex auto
        hw-id 00:21:5e:68:6f:4c
        smp_affinity auto
        speed auto
    }
    ethernet eth1 {
        address 10.12.128.1/19
        address 10.11.128.1/16
        description CnpyNet
        duplex auto
        hw-id 00:21:5e:68:6f:4d
        smp_affinity auto
        speed auto
        vif 10 {
            address 172.16.111.1/24
        }
        vif 20 {
            address 10.30.128.1/24
            address 190.14.170.1/24
            address 190.14.171.129/26
